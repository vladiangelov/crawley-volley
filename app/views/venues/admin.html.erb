<div class="container">
  <div class="admin">
    <div>
      <h1 class="page-h1">ADMIN VENUES</h1>
      <%= link_to "Create a venue", new_venue_path %>
      <% @venues.each do |venue| %>
      <p><%= venue.name %> 
      <%= link_to "Edit", edit_venue_path(venue.id) %> 
      <%= link_to "Delete", venue_path(venue.id), method: :delete %> </p>
      <% end %>
    </div>
  </div>
</div>
